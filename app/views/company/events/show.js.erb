$.magnificPopup.open({
  items: {
      src: '<%= j render "show" %>',
  },
  removalDelay: 300,
  mainClass: 'my-mfp-zoom-in',
  zoom: {
    enabled: true,
    duration: 300,
    easing: 'ease-in-out',
  },
  callbacks: {
    afterClose: function() {
      $(".daterangepicker").remove();
      window.pickr.destroyAndRemove();
    },
  }
});

$(document).ready(function() {
  registerPopupTubs();
  $('#event_ticket_id').selectpicker();
  pickrInit();
})
